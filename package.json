{
  "name": "vue-silentbox",
  "version": "3.0.4",
  "scripts": {
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "Vue",
    "Silentbox",
    "Lightbox",
    "Fancybox",
    "Vuejs",
    "Vue component",
    "Vue 3",
    "Gallery",
    "Image viewer",
    "Image slider",
    "Images",
    "Galleries",
    "Fancybox-gallery",
    "Lightbox-gallery"
  ],
  "homepage": "https://silentbox.silencesys.com/",
  "author": "Martin Roƒçek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/silencesys/silentbox/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/silencesys/silentbox.git"
  },
  "main": "dist/silent-box.umd.js",
  "module": "dist/silent-box.es.js",
  "typings": "dist/src/main.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "vue": "^3.2.40",
    "@babel/types": "^7.20.7",
    "@testing-library/vue": "^6.6.1",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.56",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitest/coverage-c8": "^0.24.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "esbuild": "^0.15.10",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "happy-dom": "^7.4.0",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "typescript": "~4.7.4",
    "vite": "^3.2.7",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.23.4",
    "vue-tsc": "^1.0.3"
  }
